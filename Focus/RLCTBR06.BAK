															"'" + a_Dados[n_Item][DCCUSTO] 		+ "'," +;
															"'" + a_Dados[n_Item][DDESCCUSTO] 	+ "'," +;
															c_AnoMes +;
															",Val('" + a_Dados[n_Item][Len(a_Dados[n_Item])] + "')" + " } )"
		&(c_ArrayXLS)

Next n_Item
	
//--------------------------------------------------------------------------------------------------------------------------------------------------------//

c_Arq := c_Nome
oExcel:Activate()
oExcel:GetXMLFile(c_Arq+c_ExtArq)
                                        
//- Move Arquivo para Pasta Relato do Usuário
c_NovoArq := AllTrim(c_Path + c_Arq + c_ExtArq)           

