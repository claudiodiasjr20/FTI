User Function NomeMes(n_Mes)

Local c_NomeMes := ""

If n_Mes >= 1 .Or. n_Mes <= 12

	a_Mes := {	"Janeiro",;
			    "Fevereiro",;
			    "Marco",;
			    "Abril",;
			    "Maio",;
			    "Junho",;
			    "Julho",;
		        "Agosto",;
		        "Setembro",;
		        "Outubro",;
		        "Novembro",;
		        "Dezembro"}
		        
	c_NomeMes := a_Mes[n_Mes]
	
Else

	c_NomeMes := "Nao Existe"
	
EndIf
              
Return c_NomeMes

